{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "types": ["jest", "node"],
    "typeRoots": ["./src/types"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "allowImportingTsExtensions": false,
    "module": "esnext",
    "skipLibCheck": true,
    "noUnusedLocals": true
  }
}
